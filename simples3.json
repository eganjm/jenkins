{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "CloudFormation Template to create an S3 Bucket",
  "Resources": {
    "MyS3Bucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": "jenkins-demo-s3-bucket-name",
        "AccessControl": "Private",
        "Tags": [
          {
            "Key": "Name",
            "Value": "My S3Bucket"
          }
        ]
      }
    }
  },
  "Outputs": {
    "BucketName": {
      "Description": "Name of the S3 bucket",
      "Value": {
        "Ref": "MyS3Bucket"
      }
    }
  }
}
